set number
set relativenumber
set ignorecase
set autoindent
set smarttab
set tabstop=4
set shiftwidth=0
set listchars=tab:>~
set clipboard=unnamedplus
set list
set title
set laststatus=0
set background=dark
syntax off
nnoremap <C-h> :wincmd h<CR>
nnoremap <C-j> :wincmd j<CR>
nnoremap <C-k> :wincmd k<CR>
nnoremap <C-l> :wincmd l<CR>
nnoremap <Leader>t :silent !dup<CR>
